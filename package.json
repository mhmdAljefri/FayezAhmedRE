{
  "name": "fayez-ahmed",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "build": "blitz build",
    "postbuild": "next-sitemap",
    "analyze": "ANALYZE=true yarn build",
    "db:m": " blitz prisma migrate save --experimental; blitz prisma migrate up --experimental;",
    "db:g": " blitz prisma generate;",
    "db:s": " blitz db seed;",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "scripts:migrateImages": "ts-node --project=tsconfig.json -O '{\"module\":\"commonjs\"}' scripts/migrate-images.ts"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
