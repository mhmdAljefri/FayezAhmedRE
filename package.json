{
  "name": "FayezAhmed",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@prisma/cli": "2.x",
    "@prisma/client": "2.x",
    "@theme-ui/presets": "0.3.0",
    "@types/theme-ui": "0.3.7",
    "blitz": "0.26.0",
    "cloudinary-core": "2.11.3",
    "cloudinary-react": "1.6.8",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "emotion-theming": "10.0.27",
    "final-form": "4.20.1",
    "final-form-arrays": "3.0.2",
    "nuka-carousel": "4.7.2",
    "react": "0.0.0-experimental-7f28234f8",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-dropzone": "11.2.4",
    "react-error-boundary": "2.x",
    "react-final-form": "6.5.2",
    "react-final-form-arrays": "3.1.3",
    "react-icons-kit": "1.3.1",
    "react-spinners": "0.9.0",
    "react-toastify": "6.1.0",
    "rebass": "4.0.7",
    "secure-password": "4.x",
    "sweetalert2": "10.10.4",
    "sweetalert2-react": "0.8.3",
    "theme-ui": "0.3.1",
    "zod": "1.x"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.x",
    "@testing-library/react": "11.x",
    "@testing-library/react-hooks": "3.x",
    "@types/jest": "26.x",
    "@types/react": "16.x",
    "@types/secure-password": "3.x",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "babel-eslint": "10.x",
    "eslint": "7.x",
    "eslint-config-react-app": "5.x",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "husky": "4.x",
    "jest": "26.x",
    "jest-environment-jsdom-fourteen": "1.x",
    "jest-watch-typeahead": "0.x",
    "lint-staged": "10.x",
    "prettier": "2.x",
    "pretty-quick": "3.x",
    "react-test-renderer": "16.x",
    "ts-jest": "26.x",
    "typescript": "4.x"
  },
  "private": true
}
